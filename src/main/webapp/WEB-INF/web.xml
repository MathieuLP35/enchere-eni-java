<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>enchere-eni-java</display-name>
  <welcome-file-list>
    <welcome-file>AccueilServlet</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>0</session-timeout>
  </session-config>
  <servlet>
    <description></description>
    <display-name>GestionUtilisateurServlet</display-name>
    <servlet-name>GestionUtilisateurServlet</servlet-name>
    <servlet-class>fr.eni.enchere.ihm.admin.GestionUtilisateurServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GestionUtilisateurServlet</servlet-name>
    <url-pattern>/GestionUtilisateurServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GestionCategorieServlet</display-name>
    <servlet-name>GestionCategorieServlet</servlet-name>
    <servlet-class>fr.eni.enchere.ihm.admin.GestionCategorieServlet</servlet-class>
  </servlet>
   <servlet-mapping>
    <servlet-name>GestionCategorieServlet</servlet-name>
    <url-pattern>/GestionCategorieServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ModifyProfilServlet</display-name>
    <servlet-name>ModifyProfilServlet</servlet-name>
    <servlet-class>fr.eni.enchere.ihm.profil.ModifyProfilServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ModifyProfilServlet</servlet-name>
    <url-pattern>/ModifyProfilServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>VenteEnchereServlet</display-name>
    <servlet-name>VenteEnchereServlet</servlet-name>
    <servlet-class>fr.eni.enchere.ihm.enchere.VenteEnchereServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VenteEnchereServlet</servlet-name>
    <url-pattern>/VenteEnchereServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>EnchereFilter</display-name>
    <filter-name>EnchereFilter</filter-name>
    <filter-class>fr.eni.enchere.ihm.filter.EnchereFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EnchereFilter</filter-name>
    <url-pattern>/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
</web-app>